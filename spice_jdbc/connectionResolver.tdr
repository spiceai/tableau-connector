<?xml version='1.0' encoding='utf-8' ?>
<tdr class='spice_jdbc'>
	<connection-resolver>
    <connection-builder>
      <script file='connectionBuilder.js'/>
    </connection-builder>
    <connection-normalizer>
      <required-attributes>
        <default-attributes>
          <default-attribute name='server'>flight.spiceai.io</default-attribute>
          <default-attribute name='port'>443</default-attribute>
        </default-attributes>
        <attribute-list>
          <attr>v-spice-product</attr>
          <attr>server</attr>
          <attr>port</attr>
          <attr>sslmode</attr>
          <attr>authentication</attr>
          <attr>password</attr>
          <attr>v-disable-cert-verification</attr>
        </attribute-list>
      </required-attributes>
    </connection-normalizer>
    <connection-properties>
      <script file='connectionProperties.js'/>
    </connection-properties>
	</connection-resolver>
</tdr>
